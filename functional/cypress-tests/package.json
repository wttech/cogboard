{
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^7.0.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1"
  },
  "scripts": {
    "cy:run": "npx cypress run",
    "cy:open": "npx cypress open",
    "cy:run:github": "npx cypress run --env configFile=github",
    "cy:lint": "eslint cypress/**/*.js",
    "cy:prettier": "prettier --write \"cypress/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wttech/cogboard"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "cypress/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
