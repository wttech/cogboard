{
  "name": "cypress-cogboard",
  "version": "1.0.0",
  "description": "Cypress automation for CogBoard development",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "cypress": "6.8.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "scripts": {
    "test": "npm install & npx cypress run --browser chrome",
    "prettier": "prettier --write \"cypress/**/*.{js,ts,json}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wttech/cogboard"
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "cypress/**/*.{js,ts,json}": [
      "pretty-quick --staged",
      "git add"
    ]
  }
}
